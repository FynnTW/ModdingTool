<UserControl x:Class="ModdingTool.View.UserControls.ToolBoxCustom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ModdingTool.View.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <ToolBarTray DockPanel.Dock="Top" HorizontalAlignment="Stretch">
            <ToolBar HorizontalAlignment="Stretch">
                <Button Command="Save" Content="Save as JSON" ></Button>
                <Button Click="Filter" Content="Filter" ></Button>
                <Button Click="Undo_Last" Content="Undo" ></Button>
            </ToolBar>
        </ToolBarTray>
    </DockPanel>
</UserControl>
